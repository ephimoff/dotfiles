{
  // Place your snippets for typescript here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  // "Print to console": {
  // 	"prefix": "log",
  // 	"body": [
  // 		"console.log('$1');",
  // 		"$2"
  // 	],
  // 	"description": "Log output to console"
  // }
  "Print to console": {
    "prefix": "clg",
    "body": ["console.log('$1', $1);$2"],
    "description": "Log output to console"
  },
  "Warn to console": {
    "prefix": "clgw",
    "body": ["console.warn(`[WARN] ${1:var} : ${${1:var}}`);"],
    "description": "Warn output to console"
  },
  "Info to console": {
    "prefix": "clgi",
    "body": ["console.info(`[INFO] ${1:var} : ${${1:var}}`);"],
    "description": "Info output to console"
  },
  "Error to console": {
    "prefix": "clge",
    "body": ["console.error(`[ERROR] ${1:err} : ${${1:err}}`);"],
    "description": "Error output to console"
  },
  "Dir to console": {
    "prefix": "clgd",
    "body": ["console.dir( $1, { depth: null } );"],
    "description": "Full object output to console"
  }
}
