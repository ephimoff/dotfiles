{
	// Place your snippets for javascript here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Print to console": {
		"prefix": "clg",
		"body": [
			"console.log('$1');",
		],
		"description": "Log output to console"
	},
	"Warn to console": {
		"prefix": "cwa",
		"body": [
			"console.warn(`[WARN] ${1:var} : ${${1:var}}`);",
		],
		"description": "Warn output to console"
	},
	"Info to console": {
		"prefix": "cwi",
		"body": [
			"console.info(`[INFO] ${1:var} : ${${1:var}}`);",
		],
		"description": "Info output to console"
	},
	"Print blah in console": {
		"prefix": "blah",
		"body": [
			"console.info('blah');",
		],
		"description": "Print blah in console"
	},
	"Error to console": {
		"prefix": "cer",
		"body": [
			"console.error(`[ERROR] ${1:err} : ${${1:err}}`);",
		],
		"description": "Error output to console"
	},
	"Require": {
		"prefix": "rqr",
		"body": [
			"require('$1');",
		],
		"description": "Adds require call"
	},
	"Module export": {
		"prefix": "mde",
		"body": [
			"module.exports = {$1};",
		],
		"description": "Default module.exports"
	},
	"Function": {
		"prefix": "fn",
		"body": [
			"function ${1:methodName}(${2:arg}) {",
				"${3:// body}",
			"}"
		],
		"description": "Default module.exports"
	},
	"Anonymous function": {
		"prefix": "afn",
		"body": [
			"function(${1:arguments}) {",
				"${2:// body}",
			"}"
		],
		"description": "Default module.exports"
	}
}